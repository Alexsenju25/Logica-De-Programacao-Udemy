Algoritmo "saida de dados"
// Disciplina   : [Linguagem e Lógica de Programação] 
// Professor   : Antonio Carlos Nicolodi 

Var
   idade : inteiro
   salario : real
   nome : caractere
   

Inicio
   idade <- 32
   salario <- 4560.9
   nome <- "Maria Silva
   
   escreval ("A funcionária ", nome, " ganha", salario, " e tem ", idade, " anos")
Fimalgoritmo